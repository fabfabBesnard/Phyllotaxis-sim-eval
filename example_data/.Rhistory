g_title<-function(a.gplot){
tmp <- ggplot_gtable(ggplot_build(a.gplot))
title_grob <- which(sapply(tmp$grobs, function(x) x$name) == "title")
title <- tmp$grobs[[title_grob]]
return(title)}
